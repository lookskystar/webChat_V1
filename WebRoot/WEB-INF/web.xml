    <url-pattern>/HttpUtil</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetMenuServlet</servlet-name>
    <url-pattern>/GetMenuServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddMenuServlet</servlet-name>
    <url-pattern>/AddMenuServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DelMenuServlet</servlet-name>
    <url-pattern>/DelMenuServlet</url-pattern>
  </servlet-mapping>
    <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
